{
  "name": "potamus-js",
  "version": "0.0.1",
  "description": "Scripts for potamus components, that will provide you a small lib bundled with material design components",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:js": "webpack test/main.js test/bundled.js --watch",
    "test:styl": "stylus -u autoprefixer-stylus -u potamus-stylus -w test/test.styl",
    "lint": "eslint components/",
    "build": "babel components -w -d bin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiamsantos/potamus-js.git"
  },
  "keywords": [
    "potamus",
    "components",
    "material",
    "design"
  ],
  "author": "Thiago Santos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thiamsantos/potamus-js/issues"
  },
  "homepage": "https://github.com/thiamsantos/potamus-js#readme",
  "devDependencies": {
    "autoprefixer-stylus": "^0.10.0",
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "eslint": "^3.4.0",
    "potamus-stylus": "0.0.2",
    "stylus": "^0.54.5",
    "webpack": "^1.13.2"
  }
}
